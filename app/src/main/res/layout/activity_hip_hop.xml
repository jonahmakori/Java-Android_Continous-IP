<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@android:color/black"
    tools:context=".HipHopActivity"
    android:id="@+id/linearLayout">

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/search"
        android:layout_width="427dp"
        android:layout_height="0dp"
        android:layout_marginBottom="672dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_conversion_absoluteHeight="69dp"
        tools:layout_conversion_absoluteWidth="350dp"
        tools:layout_editor_absoluteX="0dp">

    </com.google.android.material.textfield.TextInputLayout>

    <EditText
        android:id="@+id/search_music"
        android:layout_width="403dp"
        android:layout_height="50dp"
        android:background="@android:color/holo_blue_dark"

        android:layout_marginBottom="572dp"
        android:drawableStart="@drawable/ic_search_black_24dp"
        android:hint="@string/search"
        android:paddingRight="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_conversion_absoluteHeight="57dp"
        tools:layout_conversion_absoluteWidth="350dp"
        tools:layout_editor_absoluteX="-2dp" />
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="134dp"
        android:layout_height="62dp"
        android:layout_marginBottom="512dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/kali"
        tools:ignore="MissingConstraints"
        tools:layout_conversion_absoluteHeight="280dp"
        tools:layout_conversion_absoluteWidth="411dp"
        tools:layout_editor_absoluteX="-2dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="251dp"
        android:layout_height="55dp"
        android:layout_marginStart="152dp"
        android:layout_marginBottom="512dp"
        android:textStyle="bold"
        android:textColor="@color/colorTextIcons"
        android:background="@color/colorAccent"
        android:text="Name : Octoppizo \n Song : StayBaby \n View Lyrics"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="134dp"
        android:layout_height="70dp"
        android:layout_marginBottom="436dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/lil"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="252dp"
        android:layout_height="68dp"
        android:textColor="@color/colorTextIcons"
        android:background="@color/colorAccent"
        android:layout_marginEnd="4dp"
        android:textStyle="bold"
        android:layout_marginBottom="436dp"
        android:text="Artist : Lil Baby \n Song : Close Friends \n Got to  Lyrics"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="128dp"
        android:layout_height="100dp"
        android:layout_marginBottom="328dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/drake"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="259dp"
        android:textColor="@color/colorTextIcons"
        android:background="@color/colorAccent"
        android:layout_height="96dp"
        android:layout_marginBottom="328dp"
        android:textStyle="bold"
        android:text="  Artist : Drake \n Song : God's Plan \n Got to  Lyrics"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="131dp"
        android:layout_height="95dp"
        android:layout_marginBottom="228dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/beyonce"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="2dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="260dp"
        android:layout_height="91dp"
        android:textStyle="bold"
        android:textColor="@color/colorTextIcons"
        android:background="@color/colorAccent"
        android:layout_marginBottom="228dp"
        android:text="Artist : Beyonce ft Jay Z \n Song : Drunk In Love \n Got to  Lyrics"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="133dp"
        android:layout_height="78dp"
        android:layout_marginBottom="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/panda"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-3dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="257dp"
        android:layout_height="75dp"
        android:textStyle="bold"
        android:textColor="@color/colorTextIcons"
        android:background="@color/colorAccent"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="144dp"
        android:text="Artist : Designer \n Song : Panda panda \n Got to  Lyrics"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="129dp"
        android:layout_height="102dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:srcCompat="@drawable/law"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="261dp"
        android:layout_height="97dp"
        android:layout_marginBottom="32dp"
        android:textStyle="bold"
        android:textColor="@color/colorTextIcons"
        android:background="@color/colorAccent"
        android:text="Artist : BreedarLaw ft Benzema \n Song : Doremi \n Got to  Lyrics"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>